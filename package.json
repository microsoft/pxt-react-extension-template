{
    "name": "pxt-qrcode-generator",
    "version": "0.0.1",
    "description": "A QR code generator for MakeCode",
    "homepage": "https://pelikhan.github.io/pxt-qrcode-generator/",
    "scripts": {
        "build:dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
        "build:prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
        "start": "npm run build:dev && webpack-dev-server --open --config webpack.dev.js",
        "predeploy": "npm run build:prod",
        "deploy": "gh-pages -d dist"
    },
    "devDependencies": {
        "midiconvert": "0.4.7",
        "eventemitter3": "3.1.0",
        "gh-pages": "^2.0.1",
        "express": "^4.16.4",
        "autoprefixer": "9.3.1",
        "awesome-typescript-loader": "^5.2.1",
        "html-webpack-plugin": "^3.2.0",
        "source-map-loader": "^0.2.4",
        "webpack": "^4.26.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "3.1.10",
        "semantic-ui-css": "2.4.1",
        "typescript": "^3.1.6",
        "ts-loader": "^5.3.1",
        "tslint-microsoft-contrib": "5.2.1",
        "tslint": "5.11.0",
        "@types/react": "^16.7.7",
        "@types/react-dom": "^16.0.11"
    },
    "dependencies": {
        "@types/qrcode": "^1.3.3",
        "qrcode": "^1.3.3",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "semantic-ui-react": "0.83.0"
    }
}
